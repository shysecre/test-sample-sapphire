{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "run-s clean-builds turbo-build",
    "common": "pnpm --filter @yume/common",
    "sapphire-utilities": "pnpm --filter @yume/sapphire-utilities",
    "yume:start": "cd apps/yume-bot && pnpm start",
    "ticket:start": "cd apps/ticket-bot && pnpm start",
    "event:start": "cd apps/event-bot && pnpm start",
    "economy:start": "cd apps/economy-bot && pnpm start",
    "yume:watch": "cd apps/yume-bot && npx tsc --watch",
    "ticket:watch": "cd apps/ticket-bot && npx tsc --watch",
    "event:watch": "cd apps/event-bot && npx tsc --watch",
    "economy:watch": "cd apps/economy-bot && npx tsc --watch",
    "common:watch": "cd packages/common && npx tsc --watch",
    "sapphire-utilities:watch": "cd packages/sapphire-utilities && npx tsc --watch",
    "start:devmod": "run-p \"**:start -- --NODE_ENV=dev --NODE_CONFIG_DIR=../../config/\"",
    "start:prodmod": "run-p \"**:start -- --NODE_ENV=prod --NODE_CONFIG_DIR=../../config/\"",
    "clean-builds": "node clean-builds.js",
    "turbo-build": "turbo run build --concurrency=1"
  },
  "author": "@shysecre",
  "packageManager": "pnpm@10.7.0",
  "license": "ISC",
  "devDependencies": {
    "@sapphire/ts-config": "^5.0.1",
    "@types/config": "^3.3.5",
    "@types/node": "^22.7.4",
    "npm-run-all": "^4.1.5",
    "turbo": "^2.1.3",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@sapphire/decorators": "^6.1.0",
    "@sapphire/discord-utilities": "^3.4.1",
    "@sapphire/discord.js-utilities": "^7.3.0",
    "@sapphire/framework": "^5.2.1",
    "@sapphire/pieces": "^4.3.1",
    "@sapphire/plugin-editable-commands": "^4.0.2",
    "@sapphire/plugin-logger": "^4.0.2",
    "@sapphire/plugin-scheduled-tasks": "^10.0.1",
    "@sapphire/plugin-subcommands": "^7.0.0",
    "@sapphire/plugin-utilities-store": "^2.0.2",
    "@sapphire/ratelimits": "^2.4.9",
    "@sapphire/time-utilities": "^1.7.12",
    "@sapphire/type": "^2.5.1",
    "@sapphire/utilities": "^3.17.0",
    "@skyra/env-utilities": "^1.3.0",
    "colorette": "^2.0.20",
    "config": "^3.3.12",
    "cross-env": "^7.0.3",
    "discord.js": "^14.16.3",
    "ioredis": "^5.4.1"
  }
}
